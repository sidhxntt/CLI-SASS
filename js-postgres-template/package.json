{
  "name": "js_template",
  "version": "1.0.0",
  "main": "./src/server.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "seed": "node prisma/seeding/data.js",
    "sync-db": "npm prisma migrate dev --name migration_1",
    "dev:server": "prisma generate && nodemon --exec node ./src/server.js",
    "dev:worker:email": "nodemon --exec node ./src/utils/workers/email.js",
    "dev:worker:sms": "nodemon --exec node ./src/utils/workers/sms.js",
    "prod:server": "prisma generate && pm2-runtime start ecosystem.config.cjs",
    "dockerise": "docker compose up"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.34.10",
    "crypto-js": "^4.2.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "http-graceful-shutdown": "^3.1.14",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "pm2": "^5.4.3",
    "prom-client": "^15.1.3",
    "twilio": "^5.4.2",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4",
    "prisma": "^5.18.0"
  }
}
